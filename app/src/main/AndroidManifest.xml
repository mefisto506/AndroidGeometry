<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.micha.androidgeometry" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.micha.androidgeometry.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.micha.androidgeometry.square"
            android:label="@string/title_activity_square" >
        </activity>
        <activity
            android:name="com.example.micha.androidgeometry.rectangle"
            android:label="@string/title_activity_rectangle" >
        </activity>
        <activity
            android:name="com.example.micha.androidgeometry.circle"
            android:label="@string/title_activity_circle" >
        </activity>
        <activity
            android:name="com.example.micha.androidgeometry.triangle"
            android:label="@string/title_activity_triangle" >
        </activity>
    </application>

</manifest>
